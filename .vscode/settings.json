{
  "files.exclude": {
    ".vscode": true,
    "**/.cache": true,
    "**/*.{exe,dSYM}": true,
    "**/compile_commands.json": true,
    "**/*.part.json": true
  },
  "files.associations": {
    "*.h": "c",
    "*-schema": "json",
  },
  "json.schemas": [
    {
      "fileMatch": [
        "test/white/tokenize/data/*.json"
      ],
      "url": "./test/white/tokenize/data/miniRT-csg-schema",
    },
  ],
  "cSpell.userWords": [
    "minishell"
  ],
  "clangd.detectExtensionConflicts": false,
  "C_Cpp.autocomplete": "Disabled",
  "C_Cpp.intelliSenseEngine": "Disabled",
  "sonarlint.pathToCompileCommands": "${workspaceFolder}/compile_commands.json",
}
